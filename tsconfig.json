{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "types": ["lodash"],
    "jsx": "react",
    "strict": true,
    "typeRoots": ["node_modules/@types", "src/types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "extends": "expo/tsconfig.base"
}
